{
  "expo": {
    "name": "ClosetLink",
    "slug": "ClosetLink",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icons/app_icon.png",
    "scheme": "closetlink",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.closetlink.app",
      "appleTeamId": "9JZYRP3D46"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icons/app_icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.closetlink.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icons/app_icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icons/app_icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.270133165712-erkg39nqnv7mg2fl6ic8nlj558lvponn"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "fonts": [
      {
        "asset": "./assets/fonts/Pretendard-Bold.ttf"
      },
      {
        "asset": "./assets/fonts/Pretendard-Medium.ttf"
      },
      {
        "asset": "./assets/fonts/Pretendard-Regular.ttf"
      },
      {
        "asset": "./assets/fonts/Pretendard-Light.ttf"
      }
    ]
  }
}
